<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>ecommerce.acess.control.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertClientServlet</servlet-name>
        <servlet-class>ecommerce.client.control.InsertClientServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertProductServlet</servlet-name>
        <servlet-class>ecommerce.product.control.InsertProductServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>ecommerce.acess.control.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdminServlet</servlet-name>
        <servlet-class>ecommerce.acess.control.LoginAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertCategoryServlet</servlet-name>
        <servlet-class>ecommerce.category.control.InsertCategoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteClientServlet</servlet-name>
        <servlet-class>ecommerce.client.control.DeleteClientServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAdminServlet</servlet-name>
        <servlet-class>ecommerce.admin.control.DeleteAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlterClientServlet</servlet-name>
        <servlet-class>ecommerce.client.control.AlterClientServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitServlet</servlet-name>
        <servlet-class>ecommerce.init.control.InitServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCartItemServlet</servlet-name>
        <servlet-class>ecommerce.cart.control.AddCartItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveCartItemServlet</servlet-name>
        <servlet-class>ecommerce.cart.control.RemoveCartItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>ecommerce.upload.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowProductImageServlet</servlet-name>
        <servlet-class>ecommerce.product.control.ShowProductImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListProductsServlet</servlet-name>
        <servlet-class>ecommerce.product.control.ListProductsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListCategoriesServlet</servlet-name>
        <servlet-class>ecommerce.category.control.ListCategoriesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertClientServlet</servlet-name>
        <url-pattern>/InsertClientServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertProductServlet</servlet-name>
        <url-pattern>/InsertProductServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdminServlet</servlet-name>
        <url-pattern>/LoginAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertCategoryServlet</servlet-name>
        <url-pattern>/InsertCategoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteClientServlet</servlet-name>
        <url-pattern>/DeleteClientServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAdminServlet</servlet-name>
        <url-pattern>/DeleteAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlterClientServlet</servlet-name>
        <url-pattern>/AlterClientServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitServlet</servlet-name>
        <url-pattern>/InitServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>InitServlet</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>AddCartItemServlet</servlet-name>
        <url-pattern>/AddCartItemServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveCartItemServlet</servlet-name>
        <url-pattern>/RemoveCartItemServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowProductImageServlet</servlet-name>
        <url-pattern>/ShowProductImageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListProductsServlet</servlet-name>
        <url-pattern>/ListProductsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListCategoriesServlet</servlet-name>
        <url-pattern>/ListCategoriesServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
